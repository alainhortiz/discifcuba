                    return new Date(parseInt(year) + ',' + parseInt(mes) + ',' + parseInt(dia))
                }

                function getNacimiento(expresionString) {
                    let year = expresionString.substring(0, 2)
                    let mes = expresionString.substring(2, 4)
                    let dia = expresionString.substring(4, 6)
                    let fecha = new Date(parseInt(year) + ',' + parseInt(mes) + ',' + parseInt(dia))
                    return dia + '-' + mes + '-' + fecha.getFullYear()
                }

                function getEdad(expresionString) {
                    let hoy = new Date()
                    let fechaNacimiento = getFechaNacimiento(expresionString);
                    let edad = hoy.getFullYear() - fechaNacimiento.getFullYear()
                    let diferenciaMeses = hoy.getMonth() - fechaNacimiento.getMonth()
                    if (
                        diferenciaMeses < 0 ||
                        (diferenciaMeses === 0 && hoy.getDate() < fechaNacimiento.getDate())
                    ) {
                        edad--
                    }
                    return edad
                }

                function getParImpar(numero) {
                    let par = false
                    let n = numero.substr(numero.length - 2, 1)
                    if (n % 2 === 0) {
